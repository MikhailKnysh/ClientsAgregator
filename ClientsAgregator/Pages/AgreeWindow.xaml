<Window x:Class="ClientsAgregator.AgreeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClientsAgregator"
        mc:Ignorable="d"
        Title="AgreeWindow" Height="140" Width="150"  WindowStyle="None" WindowStartupLocation="CenterOwner" ResizeMode="NoResize" Loaded="Window_Loaded">
    <Grid Background="#FFCDCACA">
        <Label Content="Вы уверены ?
" HorizontalAlignment="Center" Margin="10,0,10,10" VerticalAlignment="Center"/>
        <Button x:Name="CancelButton" IsCancel="True" Content="Нет" HorizontalAlignment="Right" Margin="0,0,20,20" VerticalAlignment="Bottom" Width="40"/>
        <Button x:Name="AcceptButton" IsDefault="True" Content="Да" HorizontalAlignment="Left" Margin="20,0,0,20" VerticalAlignment="Bottom" Width="40" Click="AcceptButton_Click"/>


    </Grid>
</Window>
